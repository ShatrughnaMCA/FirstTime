<script type="text/javascript">
    function GetDataItem(divID, attribute) {
        return $("div#" + divID).data(attribute);
    }
    function GetSeletedSector() {
        var sect = $("#propertySectorDropdown").data("value");
        return sect;
    }
    function GetSeletedSectorForRevenue() {
        var sect = $("#RevenueChartDropdown").data("value");
        return sect;
    }
    function GetSeletedSectorForComplaints() {
        var sect = $("#ComplaintStatusDropdown").data("value");
        return sect;
    }
    function GetSeletedSectorForDefaultPayment() {
        var sect = $("#PaymentChartDropdown").data("value");
        return sect;
    }
    function GetSeletedSectorForLegal() {
        var sect = $("#ComplaintStatusDropdown1").data("value");
        return sect;
    }
    function GetSeletedSectorForRTI() {
        var sect = $("#RevenueChartDropdown1").data("value");
        return sect;
    }

    function SearchObject(Array, searchFor) {
        var self = Array;
        searchFor = searchFor;
        var result = null;
        for (var index = 0; index < self.length; index++) {
            var item = self[index];
            if (item["name"] == searchFor) {
                result = item;
                break;
            }
        };
        return result;
    }

    function PopulateDDLValues(options, divSelector) {
        var optionString = "<li><span id=\"\">All</span></li>";

        $.each(options, function (i, item) {
            optionString += '<li><span id="' + item.value + '">' + item.label + '</span></li>'
        });
        $(divSelector + " ul.dropdown-menu").html(optionString);

        $(divSelector).data("value", "0");
        $(divSelector).find('.dropdown-toggle').html('All' + ' <span class="caret"></span>');
    }
    function AttachDDLOnClick(divSelector) {
        $(divSelector + " .dropdown-menu li span").click(function () {
            var selText = $(this).text();
            $(this).parents('.btn-group').data("value", $(this).attr("id"));
            $(this).parents('.btn-group').find('.dropdown-toggle').html(selText + ' <span class="caret"></span>');
        });
    }

    window.onload = function () {
        function show_popup() {
            $('#Maindiv').removeClass("MaindivCSS");
        };
        window.setTimeout(show_popup, 1000); // 5 seconds

    }
