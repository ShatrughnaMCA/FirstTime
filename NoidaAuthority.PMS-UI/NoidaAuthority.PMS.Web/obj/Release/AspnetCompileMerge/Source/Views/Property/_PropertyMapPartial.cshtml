
<link href='//fonts.googleapis.com/css?family=Convergence|Bitter|Droid+Sans|Ubuntu+Mono' rel='stylesheet' type='text/css' />

<div id="">

    <div class="row">
        <div class="span11">
            <div class="popin">
                <div id="map">
                    <h2>Click the property to view more details</h2>
                    @*<canvas width="800" height="600"></canvas>*@
                    <object id="naimg" type="image/svg+xml" data="~/Content/Images/NAMap/NAmap.svg" width="100%" height="780px">Your browser does not support SVG</object>
                </div>
            </div>
        </div>

    </div>
</div>

<div id="myModalPropertyData" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">

    <div class="modal-dialog" style="width:40%">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Property Details</h4>
            </div>
            <div class="modal-body">
                <div id="customerinfo" ></div>
                @*Allottee Name
                Property Number:
                Sector:
                Block:
                Plot Number:
                Status:
                Allotment Date:
                Property Category:
                Customer Address:*@


                
            </div>
            <div class="modal-footer">
               
                <button type="button" style="color:green" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>

</div>
<script>
    
   


    function ParentFunction(propertyId) {


        var $modal = $('#myModalPropertyData').modal({
            show: false
        });

       

        $.ajax(
            {
                url: '/Property/PropertyDetailJson?id=' + propertyId,
                type: 'GET',
                dataType: 'json',
                success: function (data) {
                   
                    if (data) {
                        propertyDetail = data;
                        $('modal-body').empty();
                        var p_element = document.createElement('p');
                        p_element.innerHTML = "Allottee Name :".bold() + propertyDetail.Name;
                        $('.modal-body').append(p_element);
                        p_element = document.createElement('p');
                        p_element.innerHTML = "Property Number :".bold() + propertyDetail.PropertyNumber;
                        $('.modal-body').append(p_element);
                        p_element = document.createElement('p');
                        p_element.innerHTML = "Sector :".bold() + propertyDetail.Sector;
                        $('.modal-body').append(p_element);
                        p_element = document.createElement('p');
                        p_element.innerHTML = "Block :".bold() + propertyDetail.Block;
                        $('.modal-body').append(p_element);
                        p_element = document.createElement('p');
                        p_element.innerHTML = "Plot Number :".bold() + propertyDetail.PlotNumber;
                        $('.modal-body').append(p_element);
                        p_element = document.createElement('p');
                        p_element.innerHTML = "Status :".bold() + propertyDetail.Status;
                        $('.modal-body').append(p_element);
                        p_element = document.createElement('p');
                        p_element.innerHTML = "Alottment Date :".bold() + propertyDetail.AlottmentDate;
                        $('.modal-body').append(p_element);
                        p_element = document.createElement('p');
                        p_element.innerHTML = "Property Category :".bold() + propertyDetail.PropertyCategory;
                        $('.modal-body').append(p_element);
                        p_element = document.createElement('p');
                        p_element.innerHTML = "Customer Address :".bold() + propertyDetail.CustomerAddress;
                        $('.modal-body').append(p_element);

                      
                        $('.modal-body').append('<a href="/Property/PropertyDetail/' + propertyDetail.PropertyId + '" >Click here for more info</a>');

                        $modal.modal('show');

                        $('.modal').on('hidden.bs.modal', function (e) {
                            $('.modal .modal-body').empty();
                        });
                    

                    }
                   
                }

            });
       


    }


</script>


